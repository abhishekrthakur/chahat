@model List<Tasks>

<h1>login successfully</h1>

@foreach (var task in Model)
{
    <div class="card">
        <div class="card-body rounded-1 shadow">
            <div class="d-flex align-items-center">

                <p class="text-muted mb-1">@task.Title</p>
                <h4 class="mb-0">@task.AssignedUser</h4>
                <h6 class="mb-0">@task.Status</h6>
            </div>
        </div>
    </div>
}